	keywords = {Cystic fibrosis; Fractional ventilation; Hyperpolarized (129)Xe; MRI; Ventilation defect percent},
	mesh = {Adolescent; Child; Cystic Fibrosis; Disease Progression; Female; Helium; Humans; Isotopes; Magnetic Resonance Imaging; Male; Reproducibility of Results; Respiratory Function Tests; Workflow; Xenon Isotopes},
