	keywords = {Cystic fibrosis; Hyperpolarized MRI; Lung defects; MRI scan; Ventilation defect percentage},
	month = {Jan},
	pmid = {33487537},
	pst = {aheadofprint},
	title = {Comparison of Hyperpolarized {3He} and {129Xe} {MR} Imaging in Cystic Fibrosis Patients},
