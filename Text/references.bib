	keywords = {Alzheimer's Disease Neuroimaging Initiative; General intensity scale; Intensity standardization; Magnetic Resonance Imaging; Nonlinear registration},
	mesh = {Brain; Brain Mapping; Humans; Image Processing, Computer-Assisted; Magnetic Resonance Imaging},
	month = {Dec},
	number = {6},
	pages = {727-37},
	pmc = {PMC4636718},
	pmid = {25708893},
	pst = {ppublish},
	title = {Robust Intensity Standardization in Brain Magnetic Resonance Images},
	volume = {28},
	year = {2015},
