	mesh = {Adult; Asthma; Catheter Ablation; Female; Helium; Humans; Magnetic Resonance Imaging; Male; Middle Aged; Respiratory Function Tests; Tomography, X-Ray Computed; Treatment Outcome},
	month = {Jan},
